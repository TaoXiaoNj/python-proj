    "import numpy as np\n",
    "from matplotlib import pyplot as plt\n",
    "\n",
    "line = np.linspace(-3, 3, 10000)\n",
    "plt.plot(line, np.tanh(line), label='tanh')\n",
    "plt.plot(line, np.maximum(0, line), label='relu')\n",
    "plt.legend(loc='best')"
   ]
